{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */

    "incremental": true,
    "target": "ES2022",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "CommonJS",
    "baseUrl": "./",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "removeComments": true,
    "forceConsistentCasingInFileNames": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictBindCallApply": false,
    "skipLibCheck": true
  }
}
